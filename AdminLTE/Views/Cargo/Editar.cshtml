
@{
    ViewBag.Title = "Editar";
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Cargo
        <small>Editar</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/Home"><i class="fa fa-home"></i> Home</a></li>
        <li><a href="/Cargo"><i class="fa fa-table"></i> Index</a></li>
        <li class="active">Editar</li>
    </ol>
</section>
<!-- Main content -->
<section class="content" ng-app="app" ng-controller="CargoControllerNovo_Editar">
    @Html.Partial("_Novo_Editar")
    <div class="row">
        <div class="col-md-1">
            <button type="button" class="btn btn-primary" ng-disabled="form.$invalid" ng-click="Salvar(cargo)">
                <span class="fa fa-save" aria-hidden="true"></span> Salvar
            </button>
        </div>
        <div class="col-md-1">
            <button type="button" class="btn btn-success" ng-click="Voltar()">
                <span class="fa fa-arrow-circle-left" aria-hidden="true"></span> Voltar
            </button>
        </div>
    </div>
</section><!-- /.content -->

